{
  "extends": ["eslint:recommended", "google", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": "2015",
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "require-jsdoc": [
      "warn",
      {
        "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": true,
          "ClassDeclaration": true
        }
      }
    ]
  }
}
